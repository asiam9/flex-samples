<?xml version="1.0" encoding="utf-8"?>
<mx:Panel xmlns:mx="http://www.adobe.com/2006/mxml" layout="vertical" width="100%" height="100%">

	<mx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			
			protected var _gridData:ArrayCollection;
			
			public function set gridData(gridData:ArrayCollection):void {
				this._gridData = gridData;
			}
			
			[Bindable]
			public function get gridData():ArrayCollection {
				return this._gridData;
			}
		]]>
	</mx:Script>
	
</mx:Panel>
